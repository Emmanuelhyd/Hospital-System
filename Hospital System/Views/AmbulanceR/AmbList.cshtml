@model IEnumerable<Hospital_System.Models.AmbulanceDo>

@{
    Layout = null;
}

<head>
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Inter:wght@400;500;600&family=Saira:wght@500;600;700&display=swap" rel="stylesheet">

    <!-- Icon Font Stylesheet -->
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.10.0/css/all.min.css" rel="stylesheet">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.4.1/font/bootstrap-icons.css" rel="stylesheet">

    <!-- Customized Bootstrap Stylesheet -->
    <link href="~/All css/css/bootstrap.css" rel="stylesheet">

    <!-- Template Stylesheet -->
    <link href="~/All css/css/style.css" rel="stylesheet">
    <link href="~/All css/css/style-mob.css" rel="stylesheet">

    <link href="~/All css/animate/animate.css" rel="stylesheet" />
    <link href="~/All css/animate/animate.min.css" rel="stylesheet" />

    <!-- External styles for SweetAlert -->
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">
</head>

@using (Html.BeginForm())
{
    <div class="row">
        <div class="col-md-12">
            <div class="box-inn-sp">
                <div class="inn-title">
                    <li class="page-back">
                        <a href="/ReceptionAdmin/ReceptionFrontPage">
                            <i class="fa fa-backward" aria-hidden="true"></i> Back
                        </a>
                    </li>
                    <h4>Ambulance List</h4>
                </div>

                <div class="tab-inn">
                    <div class="table-responsive table-desi">
                        <table class="table table-hover">
                            <thead>
                                <tr>
                                    <th>@Html.DisplayNameFor(m => m.Id)</th>
                                    <th>@Html.DisplayNameFor(m => m.Name)</th>
                                    <th>@Html.DisplayNameFor(m => m.AmbulanceId)</th>
                                    <th>@Html.DisplayNameFor(m => m.AmbulanceStatus)</th>
                                    <th>@Html.DisplayNameFor(m => m.DriverName)</th>
                                    <th>@Html.DisplayNameFor(m => m.DriverId)</th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var ambulance in Model)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(m => ambulance.Id)</td>
                                        <td>@Html.DisplayFor(m => ambulance.Name)</td>
                                        <td>@Html.DisplayFor(m => ambulance.AmbulanceId)</td>
                                        <td>@Html.DisplayFor(m => ambulance.AmbulanceStatus)</td>
                                        <td>@Html.DisplayFor(m => ambulance.DriverName)</td>
                                        <td>@Html.DisplayFor(m => ambulance.DriverId)</td>
                                    </tr>
                                }
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

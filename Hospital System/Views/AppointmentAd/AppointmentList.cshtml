@model Hospital_System.Dash.DashboardDetails@{    ViewBag.Title = "Appointment List";    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";}<script>    function CreateNew() {        window.location.href = 'AddAppointmentAd';    }</script>@using (Html.BeginForm()){<div class="container">    <h1>Appointment List</h1>    <div class="controls">        <input type="button" value="Add New" onclick="CreateNew()" class="btn-action">        <input type="search" id="app" name="app" placeholder="Search appointments" class="form-control">        @*<input type="search" id="app" name="app" placeholder="Search appointments" class="search-input">*@        <input type="submit" value="Search" class="btn-action">    </div>    <table class="Appointment" style=" width: 100%; border-collapse: collapse; margin-top: 20px; background-color: white;">        <thead>            <tr style="padding: 12px; text-align: left; border-bottom: 1px solid #ddd;">                <th style="padding: 12px; text-align: left; border-bottom: 1px solid #ddd;">Id</th>                <th>Patient Name</th>                <th>Gender</th>                <th>Department</th>                <th>Problem</th>                <th> DoctorName</th>                <th>Phone Number</th>                <th>Address</th>                <th>Date</th>                <th>Time</th>                <th>Description</th>                <th>Action</th>            </tr>        </thead>        <tbody>            @foreach (var item in Model.mAppointmentAds)            {<tr>    <td>@Html.DisplayFor(modelItem => item.Id)</td>    <td>@Html.DisplayFor(modelItem => item.PatientName)</td>    <td>@Html.DisplayFor(modelItem => item.Gender)</td>    <td>@Html.DisplayFor(modelItem => item.Department)</td>    <td>@Html.DisplayFor(modelItem => item.Problem)</td>    <td>@Html.DisplayFor(modelItem => item.DoctorName)</td>    <td>@Html.DisplayFor(modelItem => item.PhoneNumber)</td>    <td>@Html.DisplayFor(modelItem => item.Address)</td>    <td>@Html.DisplayFor(modelItem => item.Date)</td>    <td>@Html.DisplayFor(modelItem => item.Time)</td>    <td>@Html.DisplayFor(modelItem => item.Description)</td>    <td class="action-buttons">        <a href="@Url.Action("AppointmentEdit", new { Id = item.Id })" class="edit-link">            <i class="fas fa-edit"></i>        </a>        <a href="javascript:void(0);" onclick="confirmDelete('@Url.Action("AppointmentAdDelete", new { Id = item.Id })')" class="delete-link">            <i class="fas fa-trash-alt"></i>        </a>    </td></tr>}        </tbody>    </table></div>}<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" /><link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css"><style>    .container {        background-color: white;        border-radius: 8px;        box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);        padding: 20px;        width: 85%;        margin: auto;    }    h1 {        color: #333;        text-align: center;    }    .controls {        margin-bottom: 20px;        display: flex;        justify-content: space-between;    }    .btn-action {        padding: 10px 15px;        border: none;        background-color: #3051bd;        color: white;        cursor: pointer;        border-radius: 4px;        transition: background-color 0.3s;    }        .btn-action:hover {            background-color: #253f8f;        }    .form-control {        display: block;        width: 35%;        height: calc(1.5em + .75rem + 2px);        padding: .375rem .75rem;        font-size: 1rem;        font-weight: 400;        line-height: 1.5;        color: #495057;        background-color: #fff;        background-clip: padding-box;        border: 1px solid #ced4da;        border-radius: .25rem;        transition: border-color .15s ease-in-out, box-shadow .15s ease-in-out;        .td    {        padding: 12px;        text-align: left;        border-bottom: 1px solid #ddd;    }    th {        background-color: #f2f2f2;    }    tr:hover {        background-color: #f5f5f5;    }    .action-buttons {        display: flex;        gap: 10px;    }    .edit-link {        color: blue;    }    .delete-link {        color: red;    }</style><script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script><script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script><script>    function confirmDelete(deleteUrl) {        swal({            title: "Are you sure?",            text: "You will not be able to recover this record!",            type: "warning",            showCancelButton: true,            confirmButtonColor: "#DD6B55",            confirmButtonText: "Yes, delete it!",            cancelButtonText: "No, cancel!",            closeOnConfirm: false        }, function () {            $.ajax({                url: deleteUrl,                type: 'POST',                success: function () {                    swal("Deleted!", "Your record has been deleted.", "success");                    location.reload();                },                error: function () {                    swal("Error!", "There was a problem deleting the record.", "error");                }            });        });    }</script>
<script>
    function loadAppointmentData() {
        const storedData = localStorage.getItem('appointmentData');
        if (storedData) {
            const appointmentData = JSON.parse(storedData);
            document.getElementById('name').value = appointmentData.name;
            document.getElementById('email').value = appointmentData.email;
            document.getElementById('dateInput').value = appointmentData.date;
            document.getElementById('timeInput').value = appointmentData.time;
        }
    }

    // Call the function to load data when the page is loaded
    window.onload = loadAppointmentData;
</script>



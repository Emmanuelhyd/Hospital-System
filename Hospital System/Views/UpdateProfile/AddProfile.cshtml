@model Hospital_System.Models.UpdateDO@{    ViewBag.Title = "AddProfile";    //Layout = "~/Views/Shared/_PatientLayout.cshtml";}@using (Html.BeginForm("AddProfile", "UpdateProfile")){<div class="form-row">    <div class="form-group">        @Html.LabelFor(m => m.PatientId)        @Html.TextBoxFor(m => m.PatientId, new { @placeholder = "User Name", @class = "form-control" })<br />    </div>    <div class="form-group">        @Html.LabelFor(m => m.UserName)        @Html.TextBoxFor(m => m.UserName, new { @placeholder = "User Name", @class = "form-control" })<br />    </div>    <div class="form-group">        @Html.LabelFor(m => m.FirstName)        @Html.TextBoxFor(m => m.FirstName, new { @placeholder = "First Name", @class = "form-control" })<br />    </div></div><div class="form-row">    <div class="form-group">        @Html.LabelFor(m => m.LastName)        @Html.TextBoxFor(m => m.LastName, new { @placeholder = "Last Name", @class = "form-control" })<br />    </div>    <div class="form-group">        @Html.LabelFor(m => m.Email)        @Html.TextBoxFor(m => m.Email, new { @placeholder = "Email", @class = "form-control" })<br />    </div></div><div class="form-row">    <div class="form-group">        @Html.LabelFor(m => m.BloodGroup)        @Html.TextBoxFor(m => m.BloodGroup)<br />    </div>    <div class="form-group">        @Html.LabelFor(m => m.Gender)        @Html.TextBoxFor(m => m.Gender)<br />    </div></div><div class="form-row">    <div class="form-group">        @Html.LabelFor(m => m.Age)        @Html.TextBoxFor(m => m.Age, new { @placeholder = "Age", @class = "form-control" })<br />        @Html.ValidationMessageFor(m => m.Age, "", new { @class = "text-danger" })    </div>    <div class="form-group">        @Html.LabelFor(m => m.PhoneNo)        @Html.TextBoxFor(m => m.PhoneNo, new { @class = "form-control", placeholder = "Enter Phone Number" })<br />        @Html.ValidationMessageFor(m => m.PhoneNo, "", new { @class = "text-danger" })    </div></div><div class="form-row">    <div class="form-group">        @Html.LabelFor(m => m.EmergencyContact)        @Html.TextBoxFor(m => m.EmergencyContact, new { @class = "form-control", placeholder = "Enter Emergency Contact" })<br />        @Html.ValidationMessageFor(m => m.EmergencyContact, "", new { @class = "text-danger" })    </div>    <div class="form-group">        @Html.LabelFor(m => m.Address)        @Html.TextBoxFor(m => m.Address, new { @placeholder = "Address", @class = "form-control" })<br />        @Html.ValidationMessageFor(m => m.Address, "", new { @class = "text-danger" })    </div>    <div class="form-group">        @Html.LabelFor(m => m.Type)        @Html.TextBoxFor(m => m.Type, new { @placeholder = "Address", @class = "form-control" })<br />    </div></div><div class="form-row">    <input type="submit" value="Update Profile" class="btn-submit" /></div>}
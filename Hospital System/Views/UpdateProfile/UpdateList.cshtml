ayFor(modelItem => item.Type)</td>    <td style="display: flex; gap: 10px;">        <a href="@Url.Action("ProfileEdit", new { PatientId = item.PatientId })">            <i class="fas fa-edit" style="color:blue;"></i>        </a>        <a href="@Url.Action("ProfileDelete", new { PatientId = item.PatientId })">            <i class="fas fa-trash-alt" style="color:red;"></i>        </a>    </td></tr>}        </table>    </div></div>}<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" />
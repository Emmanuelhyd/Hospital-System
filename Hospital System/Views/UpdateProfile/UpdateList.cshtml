@model Hospital_System.Dash.DashboardDetails@{    ViewBag.Title = "UpdateList";    Layout = "~/Views/Shared/_LayoutAdmin.cshtml";}<style>    body {        font-family: Arial, sans-serif;        margin: 20px;    }    table {        width: 100%;        border-collapse: collapse;        margin-top: 20px;    }    th, td {        border: 1px solid #ddd;        padding: 12px;        text-align: left;    }    th {        background-color: #f2f2f2;        color: #333;    }    tr:nth-child(even) {        background-color: #f9f9f9;    }    tr:hover {        background-color: #f1f1f1;    }    a {        text-decoration: none;        color: inherit;    }        a:hover {            opacity: 0.7;        }    .action-icons {        display: flex;        gap: 10px;    }        .action-icons i {            font-size: 18px;            cursor: pointer;        }</style>@using (Html.BeginForm()){<div>    <table>        <tr>            <th>PatientId</th>            <th>UserName</th>            <th>FirstName</th>            <th>LastName</th>            <th>Email</th>            <th>Password</th>            <th>BloodGroup</th>            <th>Gender</th>            <th>Age</th>            <th>PhoneNo</th>            <th>EmergencyContact</th>            <th>Address</th>            <th>Type</th>            <th>Action</th>        </tr>        @foreach (var item in Model.updateDOs)        {<tr>    <td>@Html.DisplayFor(modelItem => item.PatientId)</td>    <td>@Html.DisplayFor(modelItem => item.UserName)</td>    <td>@Html.DisplayFor(modelItem => item.FirstName)</td>    <td>@Html.DisplayFor(modelItem => item.LastName)</td>    <td>@Html.DisplayFor(modelItem => item.Email)</td>    <td>@Html.DisplayFor(modelItem => item.Password)</td>    <td>@Html.DisplayFor(modelItem => item.BloodGroup)</td>    <td>@Html.DisplayFor(modelItem => item.Gender)</td>    <td>@Html.DisplayFor(modelItem => item.Age)</td>    <td>@Html.DisplayFor(modelItem => item.PhoneNo)</td>    <td>@Html.DisplayFor(modelItem => item.EmergencyContact)</td>    <td>@Html.DisplayFor(modelItem => item.Address)</td>    <td>@Html.DisplayFor(modelItem => item.Type)</td>    <td class="action-icons">        <a href="@Url.Action("ProfileEdit", new { PatientId = item.PatientId })">            <i class="fas fa-edit" style="color:blue;"></i>        </a>        <a href="@Url.Action("ProfileDelete", new { PatientId = item.PatientId })">            <i class="fas fa-trash-alt" style="color:red;"></i>        </a>    </td></tr>}    </table></div>}<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" />